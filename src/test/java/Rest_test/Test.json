var schema =  {
   "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "array",
    "title": "The Root Schema",
    "items": {
      "$id": "#/items",
      "type": "object",
     "title": "The Items Schema",
     "required": [
       "code",
       "name",
       "id"
     ],
     "properties": {
       "code": {
         "$id": "#/items/properties/code",
         "type": "string",
         "title": "The Code Schema",
         "default": "",
         "examples": [								
           "DF"
         ],
         "pattern": "^(.*)$"
       },
       "name": {
         "$id": "#/items/properties/name",
         "type": "string",
         "title": "The Name Schema",
         "default": "",
         "examples": [
           "Direct Filing"
         ],
         "pattern": "^(.*)$"
       },
       "id": {
         "$id": "#/items/properties/id",
         "type": "string",
         "title": "The Id Schema",
         "default": "",
         "examples": [
          "5798961ca7c7def01d3b2885"
         ],
         "pattern": "^(.*)$"
       }
     }
   }
 }